     amort = (capex/(months*30*24)) if months and months>0 else 0.0 
     return power + amort 

 def llm_rev_per_hr(tokens_per_s, price_per_1k_tokens): 
     return tokens_per_s * 3600 * (price_per_1k_tokens/1000.0) 

