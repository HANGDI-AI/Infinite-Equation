# compute2cash.py â€” ç®—åŠ›é‡è½‰æ›å™¨ï¼ˆé›¢ç·šä¼°ç®—æ·¨åˆ©èˆ‡æ‰“å¹³å–®åƒ¹ï¼‰
import argparse

def hr_cost(watt, kwh, capex=0.0, months=0):
    power = (watt/1000.0) * kwh
    amort = (capex/(months*30*24)) if months and months>0 else 0.0
    return power + amort

def img_rev_per_hr(img_per_min, price_per_img):
    return img_per_min * 60 * price_per_img

def audio_rev_per_hr(min_per_hr, price_per_min):
    return min_per_hr * price_per_min

def llm_rev_per_hr(tokens_per_s, price_per_1k_tokens):
    return tokens_per_s * 3600 * (price_per_1k_tokens/1000.0)

def break_even_unit(cost_per_hr, units_per_hr):
    if units_per_hr <= 0: return float('inf')
    return cost_per_hr / units_per_hr

def fmt(x): return f"${x:.4f}"

if __name__ == "__main__":
    ap = argparse.ArgumentParser(description="Computeâ†’Cash ä¼°ç®—å™¨")
    ap.add_argument("--kwh", type=float, required=True, help="é›»åƒ¹ï¼ˆæ¯ kWh ç¾å…ƒï¼Œä¾‹ 0.12ï¼‰")
    ap.add_argument("--watt", type=float, required=True, help="å¹³å‡åŠŸè€—ï¼ˆç“¦ï¼‰ä¾‹å¦‚ 140")
    ap.add_argument("--capex", type=float, default=0.0, help="ç¡¬é«”æˆæœ¬ï¼ˆç¾å…ƒï¼‰")
    ap.add_argument("--months", type=int, default=0, help="ç¡¬é«”æ”¤ææœˆæ•¸ï¼ˆä¾‹ 24ï¼‰")

    # åœ–åƒä»»å‹™
    ap.add_argument("--img_per_min", type=float, default=0.0, help="æ¯åˆ†é˜å¯è™•ç†å¹¾å¼µåœ–")
    ap.add_argument("--img_price", type=float, default=0.0, help="æ¯å¼µåœ–å–®åƒ¹ï¼ˆç¾å…ƒï¼‰")

    # èªéŸ³ä»»å‹™
    ap.add_argument("--audio_min_per_hr", type=float, default=0.0, help="æ¯å°æ™‚å¯è½‰éŒ„å¹¾åˆ†é˜ï¼ˆ>60 ä»£è¡¨åŠ é€Ÿæ¯”ï¼‰")
    ap.add_argument("--audio_price_per_min", type=float, default=0.0, help="æ¯åˆ†é˜å–®åƒ¹ï¼ˆç¾å…ƒï¼‰")

    # LLM ä»»å‹™
    ap.add_argument("--llm_tokens_per_s", type=float, default=0.0, help="token/ç§’ï¼ˆè¼¸å‡º+è¼¸å…¥å¹³å‡ï¼‰")
    ap.add_argument("--llm_price_per_1k", type=float, default=0.0, help="æ¯åƒ token å–®åƒ¹ï¼ˆç¾å…ƒï¼‰")

    args = ap.parse_args()

    cost_hr = hr_cost(args.watt, args.kwh, args.capex, args.months)

    rev_img  = img_rev_per_hr(args.img_per_min, args.img_price) if args.img_per_min and args.img_price else 0.0
    rev_audio= audio_rev_per_hr(args.audio_min_per_hr, args.audio_price_per_min) if args.audio_min_per_hr and args.audio_price_per_min else 0.0
    rev_llm  = llm_rev_per_hr(args.llm_tokens_per_s, args.llm_price_per_1k) if args.llm_tokens_per_s and args.llm_price_per_1k else 0.0

    total_rev = rev_img + rev_audio + rev_llm
    net = total_rev - cost_hr

    print("=== Computeâ†’Cash ä¼°ç®— ===")
    print(f"æˆæœ¬/å°æ™‚ï¼š{fmt(cost_hr)}  ï¼ˆé›»åŠ› + æ”¤æï¼‰")
    if rev_img:
        be = break_even_unit(cost_hr, args.img_per_min*60)
        print(f"[IMG] æ”¶ç›Š/å°æ™‚ {fmt(rev_img)}ï¼›è‹¥åªåšåœ–ï¼Œæ‰“å¹³å–®åƒ¹éœ€ >= {fmt(be)} /å¼µ")
    if rev_audio:
        be = break_even_unit(cost_hr, args.audio_min_per_hr)
        print(f"[AUDIO] æ”¶ç›Š/å°æ™‚ {fmt(rev_audio)}ï¼›è‹¥åªè½‰éŒ„ï¼Œæ‰“å¹³å–®åƒ¹éœ€ >= {fmt(be)} /åˆ†é˜")
    if rev_llm:
        be = break_even_unit(cost_hr, args.llm_tokens_per_s*3600)
        print(f"[LLM] æ”¶ç›Š/å°æ™‚ {fmt(rev_llm)}ï¼›è‹¥åªåšæ¨è«–ï¼Œæ‰“å¹³å–®åƒ¹éœ€ >= {fmt(be*1000)} /åƒ token")
    print(f"â€” ç¸½æ”¶ç›Š/å°æ™‚ï¼š{fmt(total_rev)}")
    print(f"â€” æ·¨åˆ©/å°æ™‚ï¼š{fmt(net)}")
    if total_rev==0:
        print("ï¼ˆæç¤ºï¼šå°šæœªæä¾›ä»»ä½•ä»»å‹™çš„ç”¢å‡º/å–®åƒ¹åƒæ•¸ï¼‰")

