function term2specBrowser(t){
  const s=(t||'').trim().toLowerCase();
  if (termMap[s]) return termMap[s];
  if (s.includes('å“ˆå¸Œ') || s.includes('é›œæ¹Š') || s.startsWith('sha-')) return termMap["sha-256"];
  if (s.includes('rsa')) return termMap["rsa"];
